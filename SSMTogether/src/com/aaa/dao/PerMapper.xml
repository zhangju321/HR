<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aaa.dao.PerDao">
<select id="find" resultType="Person">
select * from person
</select>
<insert id="save" parameterType="Person">
insert into person(name,age,sex,birthday) value(#{name},#{age},#{sex},#{birthday})
</insert>
</mapper>